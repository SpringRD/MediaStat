            Int32 intDelayTimer;
            string strFullFileName;

            intDelayTimer = (Int32)Dts.Variables["User::DelayTimerInMs"].Value;

            strDataFilesLocation = Dts.Variables["User::FolderLocation"].Value.ToString();
            strFullFileName = Dts.Variables["User::FolderLocation"].Value.ToString() + "\\" + Dts.Variables["User::FileName"].Value.ToString();

